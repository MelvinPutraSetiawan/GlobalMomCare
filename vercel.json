{
    "version": 2,
    "builds": [
        {
            "src": "Dockerfile",
            "use": "@vercel/docker"
        }
    ],
    "routes": [{ "src": "/(.*)", "dest": "/public/index.php" }],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://global-moms-care.vercel.app",
        "LOG_CHANNEL": "stderr",
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "cookie"
    }
}
