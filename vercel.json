{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.5.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://global-moms-care.vercel.app",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "globalmomscare-globalmomscare.i.aivencloud.com",
        "DB_PORT": "24685",
        "DB_DATABASE": "globalmomscare",
        "LOG_CHANNEL": "stderr"
    }
}
